{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "indent": [
            true
        ],
        "ordered-imports": [
            true, {
                "import-sources-order": "lowercase-first",
                "named-imports-order": "lowercase-first"
            }
        ],
        "interface-name": [
            false
        ],
        "arrow-parens": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
